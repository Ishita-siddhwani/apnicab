<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="hib.beans">
    <class name="booking" table="bookingtable">
        <id name="bookingIde" column="cabid"/>
        <property name="empId" column="cabmodel"/>
        <property name="time" column="driverstatus"/>
        <property name="city" column="acfacility"/>
        <property name="area" column="area"/>
        <property name="date" column="date"/>
        
    </class>
</hibernate-mapping>



